{
  "name": "smartwaiter-server",
  "version": "2.0.0",
  "description": "smartwaiter app backend server",
  "main": "index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon ./api/api.js",
    "testing": "cross-env NODE_ENV=testing PORT=2018 nodemon ./api/api.js",
    "lint": "eslint ./api/. ./config/. ./test/.",
    "start": "npm run nodemon",
    "nodemon": "nodemon --exec npm run dev",
    "pre-commit": "npm test",
    "pretest": "",
    "production": "cross-env NODE_ENV=production node ./api/api.js",
    "test": "cross-env NODE_ENV=testing jest --detectOpenHandles ./test ./api/controllers/.",
    "test-ci": "nyc ava --no-color",
    "db:seed": "node ./mocks/seeds.js"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      [
        "@babel/transform-runtime"
      ]
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/edgarcheverier/dumbwaiter-server.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/edgarcheverier/dumbwaiter-server/issues"
  },
  "homepage": "https://github.com/edgarcheverier/dumbwaiter-server#readme",
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/plugin-transform-runtime": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "babel-polyfill": "^6.26.0",
    "cross-env": "^5.2.0",
    "eslint": "^5.0.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.6.1",
    "eslint-plugin-standard": "^3.1.0",
    "jest": "^24.3.1",
    "nodemon": "^1.17.5",
    "pre-commit": "^1.2.2",
    "prettier": "^1.13.5",
    "shx": "^0.3.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.3.4",
    "apollo-server-express": "^1.3.6",
    "atob": "^2.1.1",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.18.3",
    "cors": "^2.8.4",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "express-routes-mapper": "^1.0.2",
    "fb": "^2.0.0",
    "graphql": "^0.13.2",
    "graphql-playground-middleware-express": "^1.7.0",
    "graphql-subscriptions": "^0.5.8",
    "graphql-tools": "^3.0.2",
    "helmet": "^3.12.1",
    "jest": "^24.3.1",
    "jsonwebtoken": "^8.3.0",
    "lodash.merge": "^4.6.1",
    "morgan": "^1.9.0",
    "mysql2": "^1.5.3",
    "pre-commit": "^1.2.2",
    "randomstring": "^1.1.5",
    "sequelize": "^4.38.0",
    "subscriptions-transport-ws": "^0.9.12"
  }
}
